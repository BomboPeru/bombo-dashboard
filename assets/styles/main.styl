.rounded
  border-radius 10px
.elevation
  box-shadow 0 3px 6px rgba(0,0,0,0.16), 0 3px 6px rgba(0,0,0,0.23)

.card-container
  background #fafafa
  min-width 100px
  min-height 100px
  overflow hidden
  font-family Titillium Web

.card-header--primary
  width 100%
  height 50px
  background #445F69
  padding 0px 0px
  color #fafafa

.card-header-line
  width 100%
  height 10px

